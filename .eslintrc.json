{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": [1, 120],
        "camelcase": 0,
        "object-curly-spacing": 0,
        "quotes": 0,
        "indent": [1, 2],
        "comma-dangle": 0
    }
}
